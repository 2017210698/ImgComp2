function [code,counts] = EntropyEncodeNegs(GAMMAneg)
    m=size(GAMMAneg,1);
    n=size(GAMMAneg,2);
    
    % concat all GAMMA vals, bins+1 as separator
    GAMMACONT = [];
    for j=1:n
        for i=1:m
             GAMMACONT=[GAMMACONT ,GAMMAneg{i,j}];   %#ok<AGROW>
        end
    end
    
    % probabilties calc 0,1 only
    counts = ones(1,2); % ** initiate to ones for zeros are not allowed
  
    for i=1:length(GAMMACONT);
        counts(GAMMACONT(i)) = counts(GAMMACONT(i))+1;
    end
    
    code = arithenco(GAMMACONT,counts);
    len  = length(GAMMACONT);




end 